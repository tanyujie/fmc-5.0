<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.paradisecloud.fcm.dao.mapper.BusiFreeSwitchMapper">
    
    <resultMap type="BusiFreeSwitch" id="BusiFreeSwitchResult">
        <result property="id"    column="id"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateTime"    column="update_time"    />
        <result property="userName"    column="user_name"    />
        <result property="password"    column="password"    />
        <result property="name"    column="name"    />
        <result property="ip"    column="ip"    />
        <result property="port"    column="port"    />
        <result property="callPort"    column="call_port"    />
        <result property="domainName"    column="domain_name"    />
    </resultMap>

    <sql id="selectBusiFreeSwitchVo">
        select id, create_time, update_time, user_name, password, name, ip, port, call_port, domain_name from busi_free_switch
    </sql>

    <select id="selectBusiFreeSwitchList" parameterType="BusiFreeSwitch" resultMap="BusiFreeSwitchResult">
        <include refid="selectBusiFreeSwitchVo"/>
        <where>  
            <if test="userName != null  and userName != ''"> and user_name like concat('%', #{userName}, '%')</if>
            <if test="password != null  and password != ''"> and password = #{password}</if>
            <if test="name != null  and name != ''"> and name like concat('%', #{name}, '%')</if>
            <if test="ip != null  and ip != ''"> and ip = #{ip}</if>
            <if test="port != null "> and port = #{port}</if>
            <if test="callPort != null "> and call_port = #{callPort}</if>
            <if test="domainName != null  and domainName != ''"> and domain_name like concat('%', #{domainName}, '%')</if>
        </where>
    </select>
    
    <select id="selectBusiFreeSwitchById" parameterType="Long" resultMap="BusiFreeSwitchResult">
        <include refid="selectBusiFreeSwitchVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertBusiFreeSwitch" parameterType="BusiFreeSwitch" useGeneratedKeys="true" keyProperty="id">
        insert into busi_free_switch
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="userName != null">user_name,</if>
            <if test="password != null">password,</if>
            <if test="name != null">name,</if>
            <if test="ip != null">ip,</if>
            <if test="port != null">port,</if>
            <if test="callPort != null">call_port,</if>
            <if test="domainName != null">domain_name,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="userName != null">#{userName},</if>
            <if test="password != null">#{password},</if>
            <if test="name != null">#{name},</if>
            <if test="ip != null">#{ip},</if>
            <if test="port != null">#{port},</if>
            <if test="callPort != null">#{callPort},</if>
            <if test="domainName != null">#{domainName},</if>
         </trim>
    </insert>

    <update id="updateBusiFreeSwitch" parameterType="BusiFreeSwitch">
        update busi_free_switch
        <trim prefix="SET" suffixOverrides=",">
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="userName != null">user_name = #{userName},</if>
            <if test="password != null">password = #{password},</if>
            <if test="name != null">name = #{name},</if>
            <if test="ip != null">ip = #{ip},</if>
            <if test="port != null">port = #{port},</if>
            <if test="callPort != null">call_port = #{callPort},</if>
            <if test="domainName != null">domain_name = #{domainName},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteBusiFreeSwitchById" parameterType="Long">
        delete from busi_free_switch where id = #{id}
    </delete>

    <delete id="deleteBusiFreeSwitchByIds" parameterType="String">
        delete from busi_free_switch where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>