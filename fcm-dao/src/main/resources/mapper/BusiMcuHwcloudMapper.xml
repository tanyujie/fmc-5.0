    <delete id="deleteBusiMcuHwcloudById" parameterType="Long">
        delete from busi_mcu_hwcloud where id = #{id}
    </delete>

    <delete id="deleteBusiMcuHwcloudByIds" parameterType="String">
        delete from busi_mcu_hwcloud where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
