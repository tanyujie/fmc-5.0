            <if test="updateTime != null">update_time,</if>
            <if test="appId != null">app_id,</if>
            <if test="appKey != null">app_key,</if>
            <if test="name != null">name,</if>
            <if test="status != null">status,</if>
            <if test="capacity != null">capacity,</if>
            <if test="spareSmcId != null">spare_smc_id,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="appId != null">#{appId},</if>
            <if test="appKey != null">#{appKey},</if>
            <if test="name != null">#{name},</if>
            <if test="status != null">#{status},</if>
            <if test="capacity != null">#{capacity},</if>
            <if test="spareSmcId != null">#{spareSmcId},</if>
        </trim>
    </insert>

    <update id="updateBusiMcuHwcloud" parameterType="BusiMcuHwcloud">
        update busi_mcu_hwcloud
        <trim prefix="SET" suffixOverrides=",">
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="appId != null">app_id = #{appId},</if>
            <if test="appKey != null">app_key = #{appKey},</if>
            <if test="name != null">name = #{name},</if>
            <if test="status != null">status = #{status},</if>
            <if test="capacity != null">capacity = #{capacity},</if>
            <if test="spareSmcId != null">spare_smc_id = #{spareSmcId},</if>
