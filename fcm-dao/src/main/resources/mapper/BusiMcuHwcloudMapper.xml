    <select id="selectBusiMcuHwcloudList" parameterType="BusiMcuHwcloud" resultMap="BusiMcuHwcloudResult">
        <include refid="selectBusiMcuHwcloudVo"/>
        <where>
            <if test="appId != null  and appId != ''"> and app_id = #{appId}</if>
            <if test="appKey != null  and appKey != ''"> and app_key = #{appKey}</if>
            <if test="name != null  and name != ''"> and name like concat('%', #{name}, '%')</if>
            <if test="status != null "> and status = #{status}</if>
            <if test="capacity != null "> and capacity = #{capacity}</if>
            <if test="spareSmcId != null "> and spare_smc_id = #{spareSmcId}</if>
        </where>
    </select>

    <select id="selectBusiMcuHwcloudById" parameterType="Long" resultMap="BusiMcuHwcloudResult">
        <include refid="selectBusiMcuHwcloudVo"/>
        where id = #{id}
    </select>

    <insert id="insertBusiMcuHwcloud" parameterType="BusiMcuHwcloud" useGeneratedKeys="true" keyProperty="id">
        insert into busi_mcu_hwcloud
        <trim prefix="(" suffix=")" suffixOverrides=",">
