<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.paradisecloud.fcm.dao.mapper.BusiFreeSwitchClusterMapMapper">
    
    <resultMap type="BusiFreeSwitchClusterMap" id="BusiFreeSwitchClusterMapResult">
        <result property="id"    column="id"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateTime"    column="update_time"    />
        <result property="clusterId"    column="cluster_id"    />
        <result property="freeSwitchId"    column="free_switch_id"    />
        <result property="weight"    column="weight"    />
    </resultMap>

    <sql id="selectBusiFreeSwitchClusterMapVo">
        select id, create_time, update_time, cluster_id, free_switch_id, weight from busi_free_switch_cluster_map
    </sql>

    <select id="selectBusiFreeSwitchClusterMapList" parameterType="BusiFreeSwitchClusterMap" resultMap="BusiFreeSwitchClusterMapResult">
        <include refid="selectBusiFreeSwitchClusterMapVo"/>
        <where>  
            <if test="clusterId != null "> and cluster_id = #{clusterId}</if>
            <if test="freeSwitchId != null "> and free_switch_id = #{freeSwitchId}</if>
            <if test="weight != null "> and weight = #{weight}</if>
        </where>
    </select>
    
    <select id="selectBusiFreeSwitchClusterMapById" parameterType="Long" resultMap="BusiFreeSwitchClusterMapResult">
        <include refid="selectBusiFreeSwitchClusterMapVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertBusiFreeSwitchClusterMap" parameterType="BusiFreeSwitchClusterMap" useGeneratedKeys="true" keyProperty="id">
        insert into busi_free_switch_cluster_map
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="clusterId != null">cluster_id,</if>
            <if test="freeSwitchId != null">free_switch_id,</if>
            <if test="weight != null">weight,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="clusterId != null">#{clusterId},</if>
            <if test="freeSwitchId != null">#{freeSwitchId},</if>
            <if test="weight != null">#{weight},</if>
         </trim>
    </insert>

    <update id="updateBusiFreeSwitchClusterMap" parameterType="BusiFreeSwitchClusterMap">
        update busi_free_switch_cluster_map
        <trim prefix="SET" suffixOverrides=",">
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="clusterId != null">cluster_id = #{clusterId},</if>
            <if test="freeSwitchId != null">free_switch_id = #{freeSwitchId},</if>
            <if test="weight != null">weight = #{weight},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteBusiFreeSwitchClusterMapById" parameterType="Long">
        delete from busi_free_switch_cluster_map where id = #{id}
    </delete>

    <delete id="deleteBusiFreeSwitchClusterMapByIds" parameterType="String">
        delete from busi_free_switch_cluster_map where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>