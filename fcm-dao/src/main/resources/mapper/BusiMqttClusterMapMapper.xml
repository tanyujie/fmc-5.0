    <delete id="deleteBusiMqttClusterMapById" parameterType="Long">
        delete from busi_mqtt_cluster_map where id = #{id}
    </delete>

    <delete id="deleteBusiMqttClusterMapByIds" parameterType="String">
        delete from busi_mqtt_cluster_map where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
