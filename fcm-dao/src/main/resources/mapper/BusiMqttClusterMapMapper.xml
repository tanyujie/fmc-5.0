    </resultMap>

    <sql id="selectBusiMqttClusterMapVo">
        select id, create_time, update_time, cluster_id, mqtt_id, weight from busi_mqtt_cluster_map
    </sql>

    <select id="selectBusiMqttClusterMapList" parameterType="BusiMqttClusterMap" resultMap="BusiMqttClusterMapResult">
        <include refid="selectBusiMqttClusterMapVo"/>
        <where>  
