            <if test="mqttId != null "> and mqtt_id = #{mqttId}</if>
            <if test="weight != null "> and weight = #{weight}</if>
        </where>
    </select>
    
    <select id="selectBusiMqttClusterMapById" parameterType="Long" resultMap="BusiMqttClusterMapResult">
        <include refid="selectBusiMqttClusterMapVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertBusiMqttClusterMap" parameterType="BusiMqttClusterMap" useGeneratedKeys="true" keyProperty="id">
        insert into busi_mqtt_cluster_map
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="clusterId != null">cluster_id,</if>
            <if test="mqttId != null">mqtt_id,</if>
            <if test="weight != null">weight,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="clusterId != null">#{clusterId},</if>
            <if test="mqttId != null">#{mqttId},</if>
            <if test="weight != null">#{weight},</if>
         </trim>
    </insert>

    <update id="updateBusiMqttClusterMap" parameterType="BusiMqttClusterMap">
        update busi_mqtt_cluster_map
        <trim prefix="SET" suffixOverrides=",">
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="clusterId != null">cluster_id = #{clusterId},</if>
            <if test="mqttId != null">mqtt_id = #{mqttId},</if>
            <if test="weight != null">weight = #{weight},</if>
        </trim>
        where id = #{id}
