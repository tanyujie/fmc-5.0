<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.paradisecloud.fcm.dao.mapper.BusiTerminalSysInfoMapper">
    
    <resultMap type="BusiTerminalSysInfo" id="BusiTerminalSysInfoResult">
        <result property="id"    column="id"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateTime"    column="update_time"    />
        <result property="sn"    column="sn"    />
        <result property="macAddr"    column="mac_addr"    />
        <result property="systemVersion"    column="system_version"    />
        <result property="bootVersion"    column="boot_version"    />
        <result property="armVersion"    column="arm_version"    />
        <result property="fpgaVersion"    column="fpga_version"    />
        <result property="terminalType"    column="terminal_type"    />
    </resultMap>

    <sql id="selectBusiTerminalSysInfoVo">
        select id, create_time, update_time, sn, mac_addr, system_version, boot_version, arm_version, fpga_version, terminal_type from busi_terminal_sys_info
    </sql>
    
    <resultMap type="UpgradeTerminalType" id="TerminalTypeMap">
        <result property="terminalType"    column="terminal_type"    />
    </resultMap>
    
    <select id="getTerminalType" resultMap="TerminalTypeMap">
        SELECT sys.terminal_type FROM busi_terminal_sys_info sys GROUP BY sys.terminal_type;
    </select>

    <select id="selectBusiTerminalSysInfoList" parameterType="BusiTerminalSysInfo" resultMap="BusiTerminalSysInfoResult">
        <include refid="selectBusiTerminalSysInfoVo"/>
        <where>  
            <if test="sn != null  and sn != ''"> and sn = #{sn}</if>
            <if test="macAddr != null  and macAddr != ''"> and mac_addr = #{macAddr}</if>
            <if test="systemVersion != null  and systemVersion != ''"> and system_version = #{systemVersion}</if>
            <if test="bootVersion != null  and bootVersion != ''"> and boot_version = #{bootVersion}</if>
            <if test="armVersion != null  and armVersion != ''"> and arm_version = #{armVersion}</if>
            <if test="fpgaVersion != null  and fpgaVersion != ''"> and fpga_version = #{fpgaVersion}</if>
            <if test="terminalType != null "> and terminal_type = #{terminalType}</if>
        </where>
    </select>
    
    <select id="selectBusiTerminalSysInfoById" parameterType="Long" resultMap="BusiTerminalSysInfoResult">
        <include refid="selectBusiTerminalSysInfoVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertBusiTerminalSysInfo" parameterType="BusiTerminalSysInfo" useGeneratedKeys="true" keyProperty="id">
        insert into busi_terminal_sys_info
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="sn != null">sn,</if>
            <if test="macAddr != null">mac_addr,</if>
            <if test="systemVersion != null">system_version,</if>
            <if test="bootVersion != null">boot_version,</if>
            <if test="armVersion != null">arm_version,</if>
            <if test="fpgaVersion != null">fpga_version,</if>
            <if test="terminalType != null">terminal_type,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="sn != null">#{sn},</if>
            <if test="macAddr != null">#{macAddr},</if>
            <if test="systemVersion != null">#{systemVersion},</if>
            <if test="bootVersion != null">#{bootVersion},</if>
            <if test="armVersion != null">#{armVersion},</if>
            <if test="fpgaVersion != null">#{fpgaVersion},</if>
            <if test="terminalType != null">#{terminalType},</if>
         </trim>
    </insert>

    <update id="updateBusiTerminalSysInfo" parameterType="BusiTerminalSysInfo">
        update busi_terminal_sys_info
        <trim prefix="SET" suffixOverrides=",">
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="sn != null">sn = #{sn},</if>
            <if test="macAddr != null">mac_addr = #{macAddr},</if>
            <if test="systemVersion != null">system_version = #{systemVersion},</if>
            <if test="bootVersion != null">boot_version = #{bootVersion},</if>
            <if test="armVersion != null">arm_version = #{armVersion},</if>
            <if test="fpgaVersion != null">fpga_version = #{fpgaVersion},</if>
            <if test="terminalType != null">terminal_type = #{terminalType},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteBusiTerminalSysInfoById" parameterType="Long">
        delete from busi_terminal_sys_info where id = #{id}
    </delete>

    <delete id="deleteBusiTerminalSysInfoByIds" parameterType="String">
        delete from busi_terminal_sys_info where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>