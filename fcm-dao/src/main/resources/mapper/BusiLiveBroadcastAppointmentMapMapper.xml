<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.paradisecloud.fcm.dao.mapper.BusiLiveBroadcastAppointmentMapMapper">

    <resultMap type="BusiLiveBroadcastAppointmentMap" id="BusiLiveBroadcastAppointmentMapResult">
        <result property="id"    column="id"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateTime"    column="update_time"    />
        <result property="liveBroadcastId"    column="live_broadcast_id"    />
        <result property="mcuType"    column="mcu_type"    />
        <result property="appointmentId"    column="appointment_id"    />
    </resultMap>

    <sql id="selectBusiLiveBroadcastAppointmentMapVo">
        select id, create_time, update_time, live_broadcast_id, mcu_type, appointment_id from busi_live_broadcast_appointment_map
    </sql>

    <select id="selectBusiLiveBroadcastAppointmentMapList" parameterType="BusiLiveBroadcastAppointmentMap" resultMap="BusiLiveBroadcastAppointmentMapResult">
        <include refid="selectBusiLiveBroadcastAppointmentMapVo"/>
        <where>
            <if test="liveBroadcastId != null "> and live_broadcast_id = #{liveBroadcastId}</if>
            <if test="mcuType != null  and mcuType != ''"> and mcu_type = #{mcuType}</if>
            <if test="appointmentId != null "> and appointment_id = #{appointmentId}</if>
        </where>
    </select>

    <select id="selectBusiLiveBroadcastAppointmentMapById" parameterType="Long" resultMap="BusiLiveBroadcastAppointmentMapResult">
        <include refid="selectBusiLiveBroadcastAppointmentMapVo"/>
        where id = #{id}
    </select>

    <insert id="insertBusiLiveBroadcastAppointmentMap" parameterType="BusiLiveBroadcastAppointmentMap" useGeneratedKeys="true" keyProperty="id">
        insert into busi_live_broadcast_appointment_map
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="liveBroadcastId != null">live_broadcast_id,</if>
            <if test="mcuType != null">mcu_type,</if>
            <if test="appointmentId != null">appointment_id,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="liveBroadcastId != null">#{liveBroadcastId},</if>
            <if test="mcuType != null">#{mcuType},</if>
            <if test="appointmentId != null">#{appointmentId},</if>
        </trim>
    </insert>

    <update id="updateBusiLiveBroadcastAppointmentMap" parameterType="BusiLiveBroadcastAppointmentMap">
        update busi_live_broadcast_appointment_map
        <trim prefix="SET" suffixOverrides=",">
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="liveBroadcastId != null">live_broadcast_id = #{liveBroadcastId},</if>
            <if test="mcuType != null">mcu_type = #{mcuType},</if>
            <if test="appointmentId != null">appointment_id = #{appointmentId},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteBusiLiveBroadcastAppointmentMapById" parameterType="Long">
        delete from busi_live_broadcast_appointment_map where id = #{id}
    </delete>

    <delete id="deleteBusiLiveBroadcastAppointmentMapByIds" parameterType="String">
        delete from busi_live_broadcast_appointment_map where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>