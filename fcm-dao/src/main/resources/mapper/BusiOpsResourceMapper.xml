    <sql id="selectBusiOpsResourceVo">
        select id, sn, free_minutes, conference_number, purchase_duration, purchase_quantity, using_number, user_id, create_time, update_time, purchase_type, enable_type,used_time,mcu_type from busi_ops_resource
    </sql>

    <select id="selectBusiOpsResourceList" parameterType="BusiOpsResource" resultMap="BusiOpsResourceResult">
        <include refid="selectBusiOpsResourceVo"/>
        <where>  
            <if test="sn != null  and sn != ''"> and sn = #{sn}</if>
            <if test="freeMinutes != null "> and free_minutes = #{freeMinutes}</if>
            <if test="conferenceNumber != null "> and conference_number = #{conferenceNumber}</if>
            <if test="purchaseDuration != null "> and purchase_duration = #{purchaseDuration}</if>
            <if test="purchaseQuantity != null "> and purchase_quantity = #{purchaseQuantity}</if>
            <if test="usingNumber != null "> and using_number = #{usingNumber}</if>
            <if test="userId != null  and userId != ''"> and user_id = #{userId}</if>
            <if test="purchaseType != null  and purchaseType != ''"> and purchase_type = #{purchaseType}</if>
            <if test="enableType != null  and enableType != ''"> and enable_type = #{enableType}</if>
            <if test="usedTime != null "> and used_time = #{usedTime}</if>
            <if test="mcuType != null "> and mcu_type = #{mcuType}</if>
        </where>
    </select>
    
    <select id="selectBusiOpsResourceById" parameterType="Long" resultMap="BusiOpsResourceResult">
        <include refid="selectBusiOpsResourceVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertBusiOpsResource" parameterType="BusiOpsResource" useGeneratedKeys="true" keyProperty="id">
        insert into busi_ops_resource
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="sn != null">sn,</if>
            <if test="freeMinutes != null">free_minutes,</if>
            <if test="conferenceNumber != null">conference_number,</if>
            <if test="purchaseDuration != null">purchase_duration,</if>
            <if test="purchaseQuantity != null">purchase_quantity,</if>
            <if test="usingNumber != null">using_number,</if>
            <if test="userId != null">user_id,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="purchaseType != null">purchase_type,</if>
            <if test="enableType != null">enable_type,</if>
            <if test="usedTime != null">used_time,</if>
            <if test="mcuType != null">mcu_type,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="sn != null">#{sn},</if>
            <if test="freeMinutes != null">#{freeMinutes},</if>
            <if test="conferenceNumber != null">#{conferenceNumber},</if>
