            <if test="enableType != null">enable_type = #{enableType},</if>
            <if test="usedTime != null">used_time = #{usedTime},</if>
            <if test="mcuType != null">mcu_type = #{mcuType},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteBusiOpsResourceById" parameterType="Long">
        delete from busi_ops_resource where id = #{id}
    </delete>

    <delete id="deleteBusiOpsResourceByIds" parameterType="String">
        delete from busi_ops_resource where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>