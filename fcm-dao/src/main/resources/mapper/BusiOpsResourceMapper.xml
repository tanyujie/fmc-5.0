<mapper namespace="com.paradisecloud.fcm.dao.mapper.BusiOpsResourceMapper">
    
    <resultMap type="BusiOpsResource" id="BusiOpsResourceResult">
        <result property="id"    column="id"    />
        <result property="sn"    column="sn"    />
        <result property="freeMinutes"    column="free_minutes"    />
        <result property="conferenceNumber"    column="conference_number"    />
        <result property="purchaseDuration"    column="purchase_duration"    />
        <result property="purchaseQuantity"    column="purchase_quantity"    />
        <result property="usingNumber"    column="using_number"    />
        <result property="userId"    column="user_id"    />
