            <if test="purchaseType != null">#{purchaseType},</if>
            <if test="enableType != null">#{enableType},</if>
            <if test="usedTime != null">#{usedTime},</if>
            <if test="mcuType != null">#{mcuType},</if>
         </trim>
    </insert>

    <update id="updateBusiOpsResource" parameterType="BusiOpsResource">
        update busi_ops_resource
        <trim prefix="SET" suffixOverrides=",">
            <if test="sn != null">sn = #{sn},</if>
            <if test="freeMinutes != null">free_minutes = #{freeMinutes},</if>
            <if test="conferenceNumber != null">conference_number = #{conferenceNumber},</if>
