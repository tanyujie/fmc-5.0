<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.paradisecloud.smc.dao.model.mapper.BusiSmcHistoryConferenceMapper">

    <resultMap type="com.paradisecloud.smc.dao.model.BusiSmcHistoryConference" id="BusiSmcHistoryConferenceResult">
        <result property="id"    column="id"    />
        <result property="conferenceId"    column="conference_id"    />
        <result property="deptId"    column="dept_id"    />
        <result property="createTime"    column="create_time"    />
        <result property="subject"    column="subject"    />
        <result property="conferenceCode"    column="conference_code"    />
        <result property="endStatus"    column="end_status"    />
        <result property="endTime"    column="end_time"    />
        <result property="templateId"    column="template_id"    />
        <result property="conferenceAvcType"    column="conference_avc_type"    />
        <result property="startTime"    column="start_time"    />
        <result property="duration"    column="duration"    />
        <result property="participantNum"    column="participant_num"    />
    </resultMap>

    <sql id="selectBusiSmcHistoryConferenceVo">
        select id, conference_id, dept_id, create_time ,subject,conference_code,end_status,template_id,conference_avc_type,end_time,start_time,duration,participant_num from busi_smc_history_conference
    </sql>

    <select id="selectBusiSmcHistoryConferenceList" parameterType="com.paradisecloud.smc.dao.model.BusiSmcHistoryConference" resultMap="BusiSmcHistoryConferenceResult">
        <include refid="selectBusiSmcHistoryConferenceVo"/>
        <where>
            <if test="conferenceId != null  and conferenceId != ''"> and conference_id = #{conferenceId}</if>
            <if test="deptId != null "> and dept_id = #{deptId}</if>
            <if test="endStatus != null  and endStatus != ''"> and end_status = #{endStatus}</if>
            <if test="conferenceCode != null "> and conference_code = #{conferenceCode}</if>
            <if test="templateId != null "> and template_id = #{templateId}</if>
            <if test="conferenceAvcType != null "> and conference_avc_type = #{conferenceAvcType}</if>
            <if test="startTime!=null">
                and DATE_FORMAT(start_time,'%Y-%m-%d %H:%i:%s') <![CDATA[ >= ]]>DATE_FORMAT(#{startTime},'%Y-%m-%d %H:%i:%s')
            </if>
            <if test="endTime!=null">
                and DATE_FORMAT(end_time,'%Y-%m-%d %H:%i:%s') <![CDATA[ <= ]]>DATE_FORMAT(#{endTime},'%Y-%m-%d %H:%i:%s')
            </if>
        </where>
    </select>


    <select id="selectBusiSmcHistoryConferenceListByDate" parameterType="com.paradisecloud.smc.dao.model.BusiSmcHistoryConference" resultMap="BusiSmcHistoryConferenceResult">
        <include refid="selectBusiSmcHistoryConferenceVo"/>
        <where>
            <if test="endStatus != null  and endStatus != ''"> and end_status = #{endStatus}</if>
            <if test="startTime!=null">
                and DATE_FORMAT(start_time,'%Y-%m-%d') =DATE_FORMAT(#{startTime},'%Y-%m-%d')
            </if>
        </where>
    </select>

    <select id="selectBusiSmcHistoryConferenceById" parameterType="Integer" resultMap="BusiSmcHistoryConferenceResult">
        <include refid="selectBusiSmcHistoryConferenceVo"/>
        where id = #{id}
    </select>

    <insert id="insertBusiSmcHistoryConference" parameterType="com.paradisecloud.smc.dao.model.BusiSmcHistoryConference" useGeneratedKeys="true" keyProperty="id">
        insert into busi_smc_history_conference
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="conferenceId != null and conferenceId != ''">conference_id,</if>
            <if test="deptId != null">dept_id,</if>
            <if test="createTime != null">create_time,</if>
            <if test="subject != null">subject,</if>
            <if test="conferenceCode != null">conference_code,</if>
            <if test="endStatus != null">end_status,</if>
            <if test="endTime != null">end_time,</if>
            <if test="templateId != null">template_id,</if>
            <if test="conferenceAvcType != null">conference_avc_type,</if>
            <if test="startTime != null">start_time,</if>
            <if test="duration != null">duration,</if>
            <if test="participantNum != null">participant_num,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="conferenceId != null and conferenceId != ''">#{conferenceId},</if>
            <if test="deptId != null">#{deptId},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="subject != null">#{subject},</if>
            <if test="conferenceCode != null">#{conferenceCode},</if>
            <if test="endStatus != null">#{endStatus},</if>
            <if test="endTime != null">#{endTime},</if>
            <if test="templateId != null">#{templateId},</if>
            <if test="conferenceAvcType != null">#{conferenceAvcType},</if>
            <if test="startTime != null">#{startTime},</if>
            <if test="duration != null">#{duration},</if>
            <if test="participantNum != null">#{participantNum},</if>
        </trim>
    </insert>

    <update id="updateBusiSmcHistoryConference" parameterType="com.paradisecloud.smc.dao.model.BusiSmcHistoryConference">
        update busi_smc_history_conference
        <trim prefix="SET" suffixOverrides=",">
            <if test="conferenceId != null and conferenceId != ''">conference_id = #{conferenceId},</if>
            <if test="deptId != null">dept_id = #{deptId},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="subject != null">subject = #{subject},</if>
            <if test="conferenceCode != null">conference_code = #{conferenceCode},</if>
            <if test="endStatus != null">end_status = #{endStatus},</if>
            <if test="endTime != null">end_time = #{endTime},</if>
            <if test="templateId != null">template_id = #{templateId},</if>
            <if test="conferenceAvcType != null">conference_avc_type = #{conferenceAvcType},</if>
            <if test="startTime != null">start_time = #{startTime},</if>
            <if test="duration != null">duration = #{duration},</if>
            <if test="participantNum != null">participant_num = #{participantNum},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteBusiSmcHistoryConferenceById" parameterType="Integer">
        delete from busi_smc_history_conference where id = #{id}
    </delete>

    <delete id="deleteBusiSmcHistoryConferenceByIds" parameterType="String">
        delete from busi_smc_history_conference where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

    <select id="selectBusiSmcHistoryConferenceByConferenceId" resultMap="BusiSmcHistoryConferenceResult">
        <include refid="selectBusiSmcHistoryConferenceVo"/>
        where conference_id = #{conferenceId}
    </select>

    <select id="selectBusiSmcHistoryConferenceNotTemplate" resultMap="BusiSmcHistoryConferenceResult">
        <include refid="selectBusiSmcHistoryConferenceVo"/>
        where end_status=2
        <if test="sds != null and sds.size()>0 ">
            and dept_id in
            <foreach item="item" collection="sds" separator="," open="(" close=")" >
                  #{item.deptId}
             </foreach>
        </if>
        <if test="searchKey != null and searchKey !='' ">and (subject LIKE CONCAT('%',#{searchKey},'%') or conference_code  LIKE CONCAT('%',#{searchKey},'%'))</if>
    </select>


    <select id="selectBusiSmcHistoryConferenceListBySearchKey" resultMap="BusiSmcHistoryConferenceResult">
       select id, conference_id, dept_id, create_time ,subject,conference_code,end_status,template_id,conference_avc_type,end_time,start_time,duration,participant_num from busi_smc_history_conference
        <where>

            <if test="deptId != null "> and dept_id = #{deptId}</if>
            <if test="endStatus != null  and endStatus != ''"> and end_status = #{endStatus}</if>
            <if test="startTime!=null and startTime!='' ">
                and DATE_FORMAT(start_time,'%Y-%m-%d %H:%i:%s') <![CDATA[ >= ]]>DATE_FORMAT(#{startTime},'%Y-%m-%d %H:%i:%s')
            </if>
            <if test="endTime!=null and endTime!=''">
                and DATE_FORMAT(start_time,'%Y-%m-%d %H:%i:%s') <![CDATA[ <= ]]>DATE_FORMAT(#{endTime},'%Y-%m-%d %H:%i:%s')
            </if>
            <if test="searchKey != null and searchKey !='' ">and (subject LIKE CONCAT('%',#{searchKey},'%') or conference_code  LIKE CONCAT('%',#{searchKey},'%'))</if>
        </where>
        order by start_time desc
    </select>
</mapper>